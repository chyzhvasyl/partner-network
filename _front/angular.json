{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "partner-web-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/partner-web-app",
      "sourceRoot": "projects/partner-web-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/partner-web-app",
            "index": "projects/partner-web-app/src/index.html",
            "main": "projects/partner-web-app/src/main.ts",
            "polyfills": "projects/partner-web-app/src/polyfills.ts",
            "tsConfig": "projects/partner-web-app/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/partner-web-app/src/favicon.ico",
              "projects/partner-web-app/src/assets"
            ],
            "styles": [
              "projects/partner-web-app/src/styles.scss",
              "projects/partner-web-app/src/themes/themes.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/partner-web-app/src/environments/environment.ts",
                  "with": "projects/partner-web-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "partner-web-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "partner-web-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "partner-web-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/partner-web-app/src/test.ts",
            "polyfills": "projects/partner-web-app/src/polyfills.ts",
            "tsConfig": "projects/partner-web-app/tsconfig.spec.json",
            "karmaConfig": "projects/partner-web-app/karma.conf.js",
            "assets": [
              "projects/partner-web-app/src/favicon.ico",
              "projects/partner-web-app/src/assets"
            ],
            "styles": [
              "projects/partner-web-app/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/partner-web-app/tsconfig.app.json",
              "projects/partner-web-app/tsconfig.spec.json",
              "projects/partner-web-app/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/partner-web-app/e2e/protractor.conf.js",
            "devServerTarget": "partner-web-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "partner-web-app:serve:production"
            }
          }
        }
      }
    },
    "vendors": {
      "projectType": "library",
      "root": "projects/vendors",
      "sourceRoot": "projects/vendors/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/vendors/tsconfig.lib.json",
            "project": "projects/vendors/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/vendors/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/vendors/src/test.ts",
            "tsConfig": "projects/vendors/tsconfig.spec.json",
            "karmaConfig": "projects/vendors/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/vendors/tsconfig.lib.json",
              "projects/vendors/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "commons": {
      "projectType": "library",
      "root": "projects/commons",
      "sourceRoot": "projects/commons/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/commons/tsconfig.lib.json",
            "project": "projects/commons/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/commons/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/commons/src/test.ts",
            "tsConfig": "projects/commons/tsconfig.spec.json",
            "karmaConfig": "projects/commons/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/commons/tsconfig.lib.json",
              "projects/commons/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "translate": {
      "projectType": "library",
      "root": "projects/translate",
      "sourceRoot": "projects/translate/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/translate/tsconfig.lib.json",
            "project": "projects/translate/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/translate/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/translate/src/test.ts",
            "tsConfig": "projects/translate/tsconfig.spec.json",
            "karmaConfig": "projects/translate/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/translate/tsconfig.lib.json",
              "projects/translate/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }},
  "defaultProject": "partner-web-app"
}
